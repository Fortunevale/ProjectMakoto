{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": [
    "dev"
  ],
  "prHourlyLimit": 9,
  "bumpVersion": "prerelease",
  "packageRules": [
    {
      "groupName": "discatsharp",
      "automerge": true,
      "matchPackageNames": [
        "!/Extensions/",
        "DisCatSharp{/,}**"
      ]
    }
  ],
  "git-submodules": {
    "enabled": true
  }
}
